import json

with open('3.txt') as json_file:  
    data = json.load(json_file)
    for p in data['pltPnts']:
        print('close: ' + p['close'] + ' ')
        print('high: ' + p['high']+ ' ')
        print('low: ' + p['low']+ ' ')
        print('ltt: ' + p['ltt']+ ' ')
        print('open: ' + p['open']+ ' ')
        print('vol: ' + p['vol']+ ' ')





